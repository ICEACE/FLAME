S'<xmodel version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"\n    xsi:noNamespaceSchemaLocation=\'http://flame.ac.uk/schema/xmml_v2.xsd\'>\n    <!-- Model descriptions  -->\n    <name>ICEACE</name>\n    <version>Version 0.1.0, 29/03/2012</version>\n    <description>ICEACE - Labour Market </description>\n    <models>\n        <model>\n            <file>Market_Consumption/model_consumption.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Labour/model_labour.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Production/model_production.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Accounting/model_accounting.xml</file>\n            <enabled>true</enabled>\n        </model>\n    </models>\n    <environment>\n        <constants/>\n        <functionFiles>\n        <!-- List of C files that hold agent functions -->\n            <file>library_functions.c</file>\n            <file>firm_functions_top.c</file>\n            <file>household_functions_top.c</file>\n            <file>bank_functions_top.c</file>\n            <file>government_functions_top.c</file>\n            <file>centralbank_functions_top.c</file>\n            <file>equityfund_functions_top.c</file>\n            <file>jpoffice_functions_top.c</file>\n            <file>mall_functions_top.c</file>\n        </functionFiles>\n        <dataTypes/>\n        <timeUnits>\n            <timeUnit>\n                <name>daily</name>\n                <unit>iteration</unit>\n                <period>1</period>\n            </timeUnit>\n            <timeUnit>\n                <name>weekly</name>\n                <unit>daily</unit>\n                <period>5</period>\n            </timeUnit>\n            <timeUnit>\n                <name>monthly</name>\n                <unit>weekly</unit>\n                <period>4</period>\n            </timeUnit>\n            <timeUnit>\n                <name>quarterly</name>\n                <unit>monthly</unit>\n                <period>3</period>\n            </timeUnit>\n            <timeUnit>\n                <name>yearly</name>\n                <unit>monthly</unit>\n                <period>12</period>\n            </timeUnit>\n        </timeUnits>\n    </environment>\n\n\t<!-- List of agents -->\n    <agents>\n        <xagent>\n            <name>firm</name>\n            <description>Firm agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>isconstructor</name>\n                    <description>Identifies if the firm is a constractor firm or not.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>The day of the month to act at production and labour.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>firm_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>FirmAccountingStart</nextState>\n                </function>\n             </functions>\n        </xagent>\n        <xagent>\n            <name>household</name>\n            <description>A consumer and labourer agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>household_init</name>\n                    <description>Notifies its id..</description>\n                    <currentState>start</currentState>\n                    <nextState>HHAccountingStart</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>equityfund</name>\n            <description>Equity Fund agency.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>equityfund_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>EFAccountingStart</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>bank</name>\n            <description>A simple bank agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>bank_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>BankAccountingStart</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>government</name>\n            <description>A simple government agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>government_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>GovAccountingStart</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>centralbank</name>\n            <description>A simple central bank agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>centralbank_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>CBAccountingStart</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>jpoffice</name>\n            <description>Job placement office.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>jpoffice_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>JPOLabourMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>mall</name>\n            <description>Shopping mall.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>mall_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>MallConsumptionMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n    </agents>\n    <messages/>\n</xmodel>'
