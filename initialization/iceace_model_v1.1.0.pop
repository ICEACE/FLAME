/name>\r\n                    <description></description>\r\n                </variable>\r\n            </variables>\r\n        </message>\r\n        <message>\r\n            <name>general_benefit</name>\r\n            <description>general benefit sent to households.</description>\r\n            <variables>\r\n                <variable>\r\n                    <type>double</type>\r\n                    <name>amount</name>\r\n                    <description></description>\r\n                </variable>\r\n            </variables>\r\n        </message>\r\n        <message>\r\n            <name>fund_request</name>\r\n            <description>Amount of the liquidity requested from Equity Fund.</description>\r\n            <variables>\r\n                <variable>\r\n                    <type>int</type>\r\n                    <name>firm_id</name>\r\n                    <description></description>\r\n                </variable>\r\n                <variable>\r\n                    <type>double</type>\r\n                    <name>amount</name>\r\n                    <description></description>\r\n                </variable>\r\n            </variables>\r\n        </message>\r\n        <message>\r\n            <name>fund_request_ack</name>\r\n            <description>Amount of the liquidity requested is confirmed by Equity Fund.</description>\r\n            <variables>\r\n                <variable>\r\n                    <type>int</type>\r\n                    <name>firm_id</name>\r\n                    <description></description>\r\n                </variable>\r\n                <variable>\r\n                    <type>double</type>\r\n                    <name>amount</name>\r\n                    <description></description>\r\n                </variable>\r\n            </variables>\r\n        </message>\r\n        <message>\r\n            <name>new_entrant_loan</name>\r\n            <description>The loan requested by the new entrant, that replaces the insolvent one.</descrip