S'<xmodel version="2" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"\n    xsi:noNamespaceSchemaLocation=\'http://flame.ac.uk/schema/xmml_v2.xsd\'>\n    <!-- Model descriptions  -->\n    <name>ICEACE</name>\n    <version>Version 0.9.1, 30/08/2013</version>\n    <description>\'ICEACE - Labour Market, Production Market, Consumption Market, Credit Market and Balance Sheet Accounting </description>\n    <models>\n        <model>\n            <file>Market_Consumption/model_consumption.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Labour/model_labour.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Production/model_production.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Accounting/model_accounting.xml</file>\n            <enabled>false</enabled>\n        </model>\n        <model>\n            <file>Market_Credit/model_credit.xml</file>\n            <enabled>true</enabled>\n        </model>\n        <model>\n            <file>Market_Housing/model_housing.xml</file>\n            <enabled>true</enabled>\n        </model>\n    </models>\n    <environment>\n        <constants/>\n        <functionFiles>\n        <!-- List of C files that hold agent functions -->\n            <file>library_functions.c</file>\n            <file>firm_functions_top.c</file>\n            <file>household_functions_top.c</file>\n            <file>bank_functions_top.c</file>\n            <file>government_functions_top.c</file>\n            <file>centralbank_functions_top.c</file>\n            <file>equityfund_functions_top.c</file>\n            <file>jpoffice_functions_top.c</file>\n            <file>mall_functions_top.c</file>\n            <file>reagency_functions_top.c</file>\n        </functionFiles>\n        <dataTypes>\n            <dataType>\n                <name>transaction</name>\n                <description>A generic transaction data structure.</description>\n                <variables>\n                    <variable>\n                        <type>int</type>\n                        <name>quantity</name>\n                        <description>Number of items within a period.</description>\n                    </variable>\n                    <variable>\n                        <type>double</type>\n                        <name>avg_price</name>\n                        <description>Average price on overall transactions.</description>\n                    </variable>\n                </variables>\n            </dataType>\n        </dataTypes>\n        <timeUnits>\n            <timeUnit>\n                <name>daily</name>\n                <unit>iteration</unit>\n                <period>1</period>\n            </timeUnit>\n            <timeUnit>\n                <name>weekly</name>\n                <unit>daily</unit>\n                <period>5</period>\n            </timeUnit>\n            <timeUnit>\n                <name>monthly</name>\n                <unit>weekly</unit>\n                <period>4</period>\n            </timeUnit>\n            <timeUnit>\n                <name>quarterly</name>\n                <unit>monthly</unit>\n                <period>3</period>\n            </timeUnit>\n            <timeUnit>\n                <name>yearly</name>\n                <unit>monthly</unit>\n                <period>12</period>\n            </timeUnit>\n        </timeUnits>\n    </environment>\n\n\t<!-- List of agents -->\n    <agents>\n        <xagent>\n            <name>firm</name>\n            <description>Firm agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>Primary bank of a firm.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>isconstructor</name>\n                    <description>Identifies if the firm is a constractor firm or not.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>The day of the month to act at production and labour.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>isinsolvent</name>\n                    <description>The variable indicates if the firm is a replacement of an insolvent producer.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>firm_init</name>\n                    <description>Does run time initializations.</description>\n                    <currentState>start</currentState>\n                    <nextState>FirmCreditStart</nextState>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Skip the bank account updates.</description>\n                    <currentState>FirmUpdateDeposits</currentState>\n                    <nextState>end</nextState>\n                    <condition>\n                        <not><lhs><time>\n                            <period>weekly</period>\n                            <phase>a.day_of_week_to_act</phase>\n                        </time></lhs>\n                        <op>OR</op>\n                        <rhs><time>\n                            <period>weekly</period>\n                            <phase>1</phase>\n                        </time></rhs></not>\n                    </condition>\n                </function>\n                <function>\n                    <name>firm_update_bank_account</name>\n                    <description>Deposits/updates its liquidity to bank.</description>\n                    <currentState>FirmUpdateDeposits</currentState>\n                    <nextState>end</nextState>\n                    <condition>\n                        <lhs><time>\n                            <period>weekly</period>\n                            <phase>a.day_of_week_to_act</phase>\n                        </time></lhs>\n                        <op>OR</op>\n                        <rhs><time>\n                            <period>weekly</period>\n                            <phase>1</phase>\n                        </time></rhs>\n                    </condition>\n                    <outputs>\n                        <output>\n                            <messageName>firm_bank_update_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n             </functions>\n        </xagent>\n        <xagent>\n            <name>household</name>\n            <description>A consumer and labourer agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The ID of the bank an household has. Household applies to her bank for mortgage and deposits.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>household_init</name>\n                    <description>Notifies its id..</description>\n                    <currentState>start</currentState>\n                    <nextState>HHCreditStart</nextState>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Skip the bank account updates.</description>\n                    <currentState>HouseholdUpdateDeposits</currentState>\n                    <nextState>end</nextState>\n                    <condition>\n                        <not><lhs><time>\n                            <period>weekly</period>\n                            <phase>a.day_of_week_to_act</phase>\n                        </time></lhs>\n                        <op>OR</op>\n                        <rhs><time>\n                            <period>weekly</period>\n                            <phase>1</phase>\n                        </time></rhs></not>\n                    </condition>\n                </function>\n                <function>\n                    <name>household_update_bank_account</name>\n                    <description>Deposits/updates its liquidity to bank.</description>\n                    <currentState>HouseholdUpdateDeposits</currentState>\n                    <nextState>end</nextState>\n                    <condition>\n                        <lhs><time>\n                            <period>weekly</period>\n                            <phase>a.day_of_week_to_act</phase>\n                        </time></lhs>\n                        <op>OR</op>\n                        <rhs><time>\n                            <period>weekly</period>\n                            <phase>1</phase>\n                        </time></rhs>\n                    </condition>\n                    <outputs>\n                        <output>\n                            <messageName>household_bank_update_deposit</messageName>\n                        </output>\n                    </outputs>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>equityfund</name>\n            <description>Equity Fund agency.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>equityfund_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>EquityFundStart</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>bank</name>\n            <description>A simple bank agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Activation day.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_week_to_act</name>\n                    <description>Day of the week to act.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>bank_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>BankCreditStart</nextState>\n                </function>\n                <function>\n                    <name>idle</name>\n                    <description>Skip the bank account updates.</description>\n                    <currentState>BankUpdateDeposits</currentState>\n                    <nextState>end</nextState>\n                    <condition>\n                        <not><lhs><time>\n                            <period>weekly</period>\n                            <phase>a.day_of_week_to_act</phase>\n                        </time></lhs>\n                        <op>OR</op>\n                        <rhs><time>\n                            <period>weekly</period>\n                            <phase>1</phase>\n                        </time></rhs></not>\n                    </condition>\n                </function>\n                <function>\n                    <name>bank_update_deposits</name>\n                    <description>Deposits/updates its liquidity to bank.</description>\n                    <currentState>BankUpdateDeposits</currentState>\n                    <nextState>end</nextState>\n                    <condition>\n                        <lhs><time>\n                            <period>weekly</period>\n                            <phase>a.day_of_week_to_act</phase>\n                        </time></lhs>\n                        <op>OR</op>\n                        <rhs><time>\n                            <period>weekly</period>\n                            <phase>1</phase>\n                        </time></rhs>\n                    </condition>\n                    <inputs>\n                        <input>\n                            <messageName>household_bank_update_deposit</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                        <input>\n                            <messageName>firm_bank_update_deposit</messageName>\n                            <filter>\n                                <!-- checks self id. -->\n                                <lhs><value>a.id</value></lhs>\n                                <op>EQ</op>\n                                <rhs><value>m.bank_id</value></rhs>\n                            </filter>\n                        </input>\n                    </inputs>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>government</name>\n            <description>A simple government agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>government_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>GovernmentStart</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>centralbank</name>\n            <description>A simple central bank agent.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Day of the month to act.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>unemployment_rate</name>\n                    <description>Unemployment rate. It is updated monthly</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>consumption_goods_prices[12]</name>\n                    <description>Unemployment rate. It is updated monthly</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>centralbank_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>CentralBankCredit</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>jpoffice</name>\n            <description>Job placement office.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>jpoffice_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>JPOLabourMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>mall</name>\n            <description>Shopping mall.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>mall_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>MallConsumptionMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n        <xagent>\n            <name>reagency</name>\n            <description>Real Estate Agency.</description>\n            <memory>\n                <variable>\n                    <type>int</type>\n                    <name>id</name>\n                    <description>Agent ID.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>day_of_month_to_act</name>\n                    <description>Activation day.</description>\n                </variable>\n                <variable>\n                    <type>int</type>\n                    <name>it_no</name>\n                    <description>Current iteration. Incremented daily bais.</description>\n                </variable>\n            </memory>\n            <functions>\n                <function>\n                    <name>reagency_init</name>\n                    <description>Notifies its id.</description>\n                    <currentState>start</currentState>\n                    <nextState>REAgencyHousingMarket</nextState>\n                </function>\n            </functions>\n        </xagent>\n    </agents>\n    <messages>\n        <message>\n            <name>firm_bank_update_deposit</name>\n            <description>Firm sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>household_bank_update_deposit</name>\n            <description>Household sends out its liquid money to bank.</description>\n            <variables>\n                <variable>\n                    <type>int</type>\n                    <name>bank_id</name>\n                    <description>The bank where liquid money to be deposited.</description>\n                </variable>\n                <variable>\n                    <type>double</type>\n                    <name>amount</name>\n                    <description>Amount of liquid money to be deposited.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>interest_rate</name>\n            <description>The interest rate sent by the central bank.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>rate</name>\n                    <description>The rate.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>capital_tax_rate</name>\n            <description>Government tax rate on capital income.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>value</name>\n                    <description>Rate of the tax.</description>\n                </variable>\n            </variables>\n        </message>\n        <message>\n            <name>labour_tax_rate</name>\n            <description>Government tax rate on labour income.</description>\n            <variables>\n                <variable>\n                    <type>double</type>\n                    <name>value</name>\n                    <description>Rate of the tax.</description>\n                </variable>\n            </variables>\n        </message>\n    </messages>\n</xmodel>'
